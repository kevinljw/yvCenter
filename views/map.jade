extends layout

block content
  .wrapper
    .toypf-background
      .filter-black
    .profile-content.section-nude
      .container
        .row.owner
          .col-md-2.col-md-offset-5.col-sm-4.col-sm-offset-4.col-xs-6.col-xs-offset-3.text-center
            .avatar
              img.img-circle.img-no-padding.img-responsive(src='/img/yvcLogo.png', alt='')
          .col-md-4.col-md-offset-4.col-sm-4.col-sm-offset-4.col-xs-4.col-xs-offset-4.text-center
            .name
              h4
                | 我們在這裡
              h5
                | 北基金青年志工中心
                br
                | 交通資訊
          img.img-no-padding.col-md-offset-3.col-md-6.col-sm-8.col-sm-offset-2.col-xs-10.col-xs-offset-1(src='/img/toypf_map-01.png', alt='')
        .profile-tabs
          .nav-tabs-navigation
            .nav-tabs-wrapper
              ul.nav.nav-tabs#tabs(data-tabs="tabs")
                li.active
                  a(href="#bus" data-toggle="tab")
                    i.fa.fa-bus
                    | 公車
                li
                  a(href="#train" data-toggle="tab")
                    i.fa.fa-train
                    | 火車
                li
                  a(href="#mrt" data-toggle="tab")
                    i.fa.fa-subway
                    | 捷運
                li
                  a(href="#car" data-toggle="tab")
                    i.fa.fa-automobile
                    | 開車
